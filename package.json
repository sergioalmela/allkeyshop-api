{
  "name": "allkeyshop-api",
  "version": "1.2.0",
  "description": "Unofficial Allkeyshop API",
  "main": "dist/src/allkeyshop.js",
  "types": "dist/src/allkeyshop.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint --ext .ts src/ tests/",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run build && jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sergioalmela/allkeyshop-api"
  },
  "keywords": [
    "allkeyshop.com",
    "www.allkeyshop.com",
    "api"
  ],
  "author": "Sergio Almela",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/sergioalmela/allkeyshop-api/issues"
  },
  "homepage": "https://github.com/sergioalmela/allkeyshop-api",
  "dependencies": {
    "axios": "^1.6.8",
    "fuzzy-search": "^3.2.1"
  },
  "devDependencies": {
    "@types/fuzzy-search": "^2.1.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
